{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { onMounted, reactive, ref } from 'vue';\nexport default {\n  __name: 'AddMenus',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const props = {\n      id: 'id',\n      label: 'label',\n      children: 'subMenus'\n    };\n    const treeNodeList = ref([]);\n    const menuForm = reactive({\n      label: ''\n    });\n    function loadMenuTree() {\n      axios.get(\"http://localhost:8081/listMenus\").then(response => {\n        treeNodeList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    onMounted(function () {\n      loadMenuTree();\n    });\n    var id = 0;\n    function hanldNodeClick(node) {\n      console.log(node);\n      id = node.id;\n    }\n    function subMenuForm() {\n      if (!isEditMode.value && id == 0) {\n        ElMessage(\"请选择当前新增菜单节点的父节点\");\n        return;\n      }\n      const url = isEditMode.value ? `http://localhost:8081/updateMenu/${currentEditId.value}` : \"http://localhost:8081/saveMenus\";\n      const data = isEditMode.value ? {\n        label: menuForm.label\n      } : {\n        ...menuForm,\n        pid: id\n      };\n      const method = isEditMode.value ? 'put' : 'post';\n      axios[method](url, data).then(response => {\n        if (response.data.code == 200) {\n          loadMenuTree();\n          resetForm();\n        }\n        ElMessage(response.data.msg);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const isEditMode = ref(false);\n    const currentEditId = ref(0);\n    function editMenu(data) {\n      isEditMode.value = true;\n      currentEditId.value = data.id;\n      menuForm.label = data.label;\n      ElMessage('正在修改菜单，请在下方修改菜单名称');\n    }\n    function delMenus(node, data) {\n      console.log(data.subMenus);\n      if (data.subMenus.length == 0) {\n        let mid = data.id;\n        axios.get(\"http://localhost:8081/deleteMenus/\" + mid).then(response => {\n          if (response.data.code == 200) {\n            loadMenuTree();\n          }\n          ElMessage(response.data.msg);\n        });\n      } else {\n        ElMessage(data.label + \",节点存在子节点不能删除!\");\n      }\n    }\n    function resetForm() {\n      isEditMode.value = false;\n      currentEditId.value = 0;\n      menuForm.label = '';\n    }\n    const __returned__ = {\n      props,\n      treeNodeList,\n      menuForm,\n      loadMenuTree,\n      get id() {\n        return id;\n      },\n      set id(v) {\n        id = v;\n      },\n      hanldNodeClick,\n      subMenuForm,\n      isEditMode,\n      currentEditId,\n      editMenu,\n      delMenus,\n      resetForm,\n      get axios() {\n        return axios;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      onMounted,\n      reactive,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","ElMessage","onMounted","reactive","ref","props","id","label","children","treeNodeList","menuForm","loadMenuTree","get","then","response","value","data","catch","error","console","log","hanldNodeClick","node","subMenuForm","isEditMode","url","currentEditId","pid","method","code","resetForm","msg","editMenu","delMenus","subMenus","length","mid"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/AddMenus.vue"],"sourcesContent":["<template>\r\n  <h2>添加菜单</h2>\r\n  <div>\r\n    选择新增节点的父节点：\r\n      <el-tree\r\n      :props=\"props\"\r\n      :data=\"treeNodeList\"\r\n      node-key=\"id\"\r\n      ref=\"treeRef\"\r\n      @node-click=\"hanldNodeClick\"\r\n      @check-change=\"hanldNodeChange\"\r\n  >\r\n          <template #default=\"{ node, data }\">\r\n        <div class=\"custom-tree-node\">\r\n          <span>{{ node.label }}</span>\r\n          <div>\r\n            <el-button type=\"primary\" link @click=\"editMenu(data)\">\r\n              修改\r\n            </el-button>\r\n            <el-button\r\n              style=\"margin-left: 4px\"\r\n              type=\"danger\"\r\n              link\r\n              @click=\"delMenus(node, data)\"\r\n            >\r\n              删除\r\n            </el-button>\r\n          </div>\r\n        </div>\r\n      </template>\r\n\r\n    </el-tree>\r\n  </div>\r\n    <hr/>\r\n\r\n  <el-form :model=\"menuForm\" label-width=\"auto\" style=\"max-width: 600px\">\r\n  <el-form-item label=\"菜单名称\" style=\"margin-bottom: 24px\">\r\n    <el-input \r\n      v-model=\"menuForm.label\" />\r\n  </el-form-item>\r\n  <el-form-item style=\"text-align: right; margin-top: 24px\">\r\n    <el-button type=\"primary\" @click=\"subMenuForm\">{{ isEditMode ? '保存修改' : '确定' }}</el-button>\r\n    <el-button @click=\"resetForm\">取消</el-button>\r\n  </el-form-item>\r\n</el-form>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\nimport { onMounted, reactive, ref } from 'vue';\r\n\r\n\r\nconst props = {\r\n  id: 'id',\r\n  label: 'label',\r\n  children: 'subMenus'\r\n}\r\n\r\nconst treeNodeList = ref([])\r\nconst menuForm = reactive({\r\n    label:''\r\n})\r\n\r\nfunction loadMenuTree() {\r\n  axios.get(\"http://localhost:8081/listMenus\")\r\n  .then((response)=>{\r\n    treeNodeList.value = response.data\r\n  })\r\n  .catch((error)=>{\r\n    console.log(error)\r\n  })\r\n}\r\n\r\nonMounted(function(){\r\n  loadMenuTree()\r\n})\r\n\r\nvar id=0\r\n\r\nfunction hanldNodeClick(node){\r\n    console.log(node)\r\n    id=node.id\r\n}\r\n\r\nfunction subMenuForm(){\r\n    if(!isEditMode.value && id==0){\r\n        ElMessage(\"请选择当前新增菜单节点的父节点\")\r\n        return\r\n    }\r\n    \r\n    const url = isEditMode.value \r\n      ? `http://localhost:8081/updateMenu/${currentEditId.value}`\r\n      : \"http://localhost:8081/saveMenus\"\r\n      \r\n    const data = isEditMode.value\r\n      ? {label: menuForm.label}\r\n      : {...menuForm, pid: id}\r\n\r\n    const method = isEditMode.value ? 'put' : 'post'\r\n\r\n    axios[method](url, data)\r\n    .then((response)=>{\r\n     if(response.data.code==200){\r\n        loadMenuTree()\r\n        resetForm()\r\n     }\r\n     ElMessage(response.data.msg)\r\n    })\r\n    .catch((error)=>{\r\n        console.log(error)\r\n    })\r\n}\r\n\r\n\r\nconst isEditMode = ref(false)\r\nconst currentEditId = ref(0)\r\n\r\nfunction editMenu(data) {\r\n  isEditMode.value = true\r\n  currentEditId.value = data.id\r\n  menuForm.label = data.label\r\n  ElMessage('正在修改菜单，请在下方修改菜单名称')\r\n}\r\n\r\nfunction delMenus(node,data){\r\n    console.log(data.subMenus)\r\n    if(data.subMenus.length==0){\r\n        let mid=data.id\r\n        axios.get(\"http://localhost:8081/deleteMenus/\"+mid)\r\n        .then((response)=>{\r\n            if(response.data.code==200){\r\n                loadMenuTree()\r\n            }\r\n            ElMessage(response.data.msg)\r\n        })\r\n    }else{\r\n        ElMessage(data.label+\",节点存在子节点不能删除!\")\r\n    }\r\n}\r\n\r\nfunction resetForm() {\r\n  isEditMode.value = false\r\n  currentEditId.value = 0\r\n  menuForm.label = ''\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.custom-tree-node {\r\n  flex: 1;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  font-size: 14px;\r\n  padding-right: 8px;\r\n}\r\n</style>"],"mappings":"AAgDA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,cAAc;AACxC,SAASC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAQ,KAAK;;;;;;;IAG9C,MAAMC,KAAK,GAAG;MACZC,EAAE,EAAE,IAAI;MACRC,KAAK,EAAE,OAAO;MACdC,QAAQ,EAAE;IACZ,CAAC;IAED,MAAMC,YAAY,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMM,QAAQ,GAAGP,QAAQ,CAAC;MACtBI,KAAK,EAAC;IACV,CAAC,CAAC;IAEF,SAASI,YAAYA,CAAA,EAAG;MACtBX,KAAK,CAACY,GAAG,CAAC,iCAAiC,CAAC,CAC3CC,IAAI,CAAEC,QAAQ,IAAG;QAChBL,YAAY,CAACM,KAAK,GAAGD,QAAQ,CAACE,IAAI;MACpC,CAAC,CAAC,CACDC,KAAK,CAAEC,KAAK,IAAG;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;IAEAhB,SAAS,