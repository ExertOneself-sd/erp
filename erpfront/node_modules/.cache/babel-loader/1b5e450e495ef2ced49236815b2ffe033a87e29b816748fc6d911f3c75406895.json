{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport { ElMessage } from \"element-plus\";\nimport { getCurrentInstance } from \"vue\";\nexport default {\n  __name: 'login',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const axios = getCurrentInstance().proxy.axios;\n    const router = useRouter();\n    const loginForm = ref();\n    const loginUser = reactive({\n      uname: '',\n      upwd: ''\n    });\n    function userLogin() {\n      axios.post(\"http://localhost:8081/userLogin\", loginUser).then(response => {\n        if (response.data.code == 200) {\n          router.push({\n            name: 'index'\n          });\n        }\n        ElMessage(response.data.msg);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const __returned__ = {\n      axios,\n      router,\n      loginForm,\n      loginUser,\n      userLogin,\n      reactive,\n      ref,\n      get useRouter() {\n        return useRouter;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      getCurrentInstance\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","ref","useRouter","ElMessage","getCurrentInstance","axios","proxy","router","loginForm","loginUser","uname","upwd","userLogin","post","then","response","data","code","push","name","msg","catch","error","console","log"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <el-card class=\"box-card\" shadow=\"always\">\r\n      <div\r\n        class=\"login-title\"\r\n      >ERP系统\r\n      </div>\r\n      <el-form\r\n        ref=\"loginForm\"\r\n        :model=\"loginUser\"\r\n        size=\"large\"\r\n        label-position=\"right\"\r\n        label-width=\"70px\"\r\n        style=\"width: 360px; margin: 0px auto\"\r\n      >\r\n        <el-form-item label=\"账号\">\r\n          <el-input v-model=\"loginUser.uname\" prefix-icon=\"el-icon-user\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input type=\"password\" v-model=\"loginUser.upwd\" show-password prefix-icon=\"el-icon-lock\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div class=\"login-button-container\">\r\n        <el-button\r\n          @click=\"userLogin\"\r\n          type=\"primary\"\r\n          size=\"large\"\r\n          class=\"login-button\"\r\n          >登录</el-button>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, ref } from \"vue\";\r\nimport { useRouter } from \"vue-router\"\r\nimport { ElMessage } from \"element-plus\";\r\nimport { getCurrentInstance } from \"vue\";\r\nconst axios=getCurrentInstance().proxy.axios\r\n\r\nconst router=useRouter()\r\nconst loginForm=ref()\r\n\r\nconst loginUser=reactive({\r\n    uname:'',\r\n    upwd:''\r\n})\r\n\r\nfunction userLogin(){\r\n    axios.post(\"http://localhost:8081/userLogin\",loginUser)\r\n    .then((response)=>{\r\n        if(response.data.code==200){\r\n            router.push({name:'index'})\r\n        }\r\n        ElMessage(response.data.msg)\r\n    })\r\n    .catch((error)=>{\r\n        console.log(error)\r\n    })\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 全局背景样式 */\r\n.login {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 20px;\r\n  background: linear-gradient(135deg, #f5f7fa 0%, #c3cfe2 100%);\r\n}\r\n\r\n/* 卡片优化 */\r\n.box-card {\r\n  width: 100%;\r\n  max-width: 400px;\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n  box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.box-card:hover {\r\n  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.15);\r\n}\r\n\r\n/* 标题样式优化 */\r\n.login-title {\r\n  padding: 40px 0 30px;\r\n  text-align: center;\r\n  color: #165DFF;\r\n  font-size: 32px;\r\n  font-weight: 600;\r\n  letter-spacing: 1px;\r\n  position: relative;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.login-title::after {\r\n  content: '';\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  width: 60px;\r\n  height: 3px;\r\n  background: linear-gradient(to right, #165DFF, #409EFF);\r\n  border-radius: 3px;\r\n}\r\n\r\n/* 表单样式优化 */\r\n.el-form {\r\n  padding: 0 30px 20px;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 25px;\r\n}\r\n\r\n.el-input {\r\n  border-radius: 8px;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.el-input:focus-within .el-input__inner {\r\n  box-shadow: 0 0 0 2px rgba(22, 93, 255, 0.2);\r\n  border-color: #165DFF;\r\n}\r\n\r\n.el-input__prefix {\r\n  color: #8c939d;\r\n}\r\n\r\n/* 按钮样式优化 */\r\n.login-button-container {\r\n  text-align: center;\r\n  padding: 0 30px 30px;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  height: 50px;\r\n  border-radius: 8px;\r\n  font-size: 16px;\r\n  font-weight: 500;\r\n  transition: all 0.3s ease;\r\n  background-color: #165DFF;\r\n  border-color: #165DFF;\r\n}\r\n\r\n.login-button:hover {\r\n  background-color: #0d47a1;\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 4px 12px rgba(22, 93, 255, 0.3);\r\n}\r\n\r\n/* 响应式设计 */\r\n@media (max-width: 420px) {\r\n  .box-card {\r\n    max-width: 340px;\r\n  }\r\n  \r\n  .el-form, .login-button-container {\r\n    padding: 0 20px 20px;\r\n  }\r\n  \r\n  .login-title {\r\n    font-size: 28px;\r\n    padding: 30px 0 20px;\r\n  }\r\n}\r\n\r\n/* 加载动画（可配合脚本使用） */\r\n.login-loading {\r\n  animation: pulse 1.5s infinite;\r\n}\r\n\r\n@keyframes pulse {\r\n  0% { transform: scale(1); }\r\n  50% { transform: scale(1.05); }\r\n  100% { transform: scale(1); }\r\n}\r\n</style>"],"mappings":";AAmCA,SAASA,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AACnC,SAASC,SAAS,QAAQ,YAAY;AACtC,SAASC