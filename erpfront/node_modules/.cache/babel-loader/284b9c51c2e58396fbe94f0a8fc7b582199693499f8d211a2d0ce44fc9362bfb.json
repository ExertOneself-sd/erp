shadow: 0 15px 35px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n/* 标题样式 */\r\n.login-title {\r\n  text-align: center;\r\n  padding: 40px 0 30px;\r\n  color: #165DFF;\r\n  font-size: 32px;\r\n  font-weight: bold;\r\n  letter-spacing: 2px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.login-title i {\r\n  margin-right: 10px;\r\n  font-size: 36px;\r\n}\r\n\r\n/* 表单样式 */\r\n.login-form {\r\n  padding: 0 30px 30px;\r\n}\r\n\r\n/* 输入框样式 */\r\n.login-input {\r\n  border-radius: 8px;\r\n  height: 50px;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.login-input:focus-within {\r\n  .el-input__inner {\r\n    border-color: #165DFF;\r\n    box-shadow: 0 0 0 2px rgba(22, 93, 255, 0.2);\r\n  }\r\n}\r\n\r\n/* 按钮样式 */\r\n.login-button-container {\r\n  padding: 0 30px 30px;\r\n}\r\n\r\n.login-button {\r\n  height: 50px;\r\n  border-radius: 8px;\r\n  background: linear-gradient(135deg, #165DFF 0%, #367AF6 100%);\r\n  border: none;\r\n  font-size: 16px;\r\n  transition: all 0.3s ease;\r\n  box-shadow: 0 4px 12px rgba(22, 93, 255, 0.3);\r\n}\r\n\r\n.login-button:hover {\r\n  background: linear-gradient(135deg, #0F4CD9 0%, #2965E0 100%);\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 6px 15px rgba(22, 93, 255, 0.4);\r\n}\r\n\r\n.login-button:active {\r\n  transform: translateY(0);\r\n}\r\n</style>\r\n"],"mappings":";AAkDA,SAASA,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AACnC,SAASC,SAAS,QAAQ,YAAY;AACtC,SAASC,SAAS,QAAQ,cAAc;AACxC,SAASC,kBAAkB,QAAQ,KAAK;;;;;;;IAExC,MAAMC,MAAM,GAAGH,SAAS,CAAC,CAAC;IAC1B,MAAMI,SAAS,GAAGL,GAAG,CAAC,CAAC;IACvB,MAAMM,KAAK,GAAGH,kBAAkB,CAAC,CAAC,CAACI,KAAK,CAACD,KAAK;IAE9C,MAAME,SAAS,GAAGT,QAAQ,CAAC;MACzBU,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;IACR,CAAC,CAAC;IAEF,SAASC,SAASA,CAAA,EAAG;MACnBL,KAAK,CAACM,IAAI,CAAC,iCAAiC,EAAEJ,SAAS,CAAC,CACrDK,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACC,IAAI,CAACC,IAAI,IAAI,GAAG,EAAE;UAC7BZ,MAAM,CAACa,IAAI,CAAC;YAAEC,IAAI,EAAE;UAAQ,CAAC,CAAC;QAChC;QACAhB,SAAS,CAACY,QAAQ,CAACC,IAAI,CAACI,GAAG,CAAC;MAC9B,CAAC,CAAC,CACDC,K