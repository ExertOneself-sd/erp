ransform: translateX(-50%);\r\n  width: 60px;\r\n  height: 3px;\r\n  background: linear-gradient(to right, #165DFF, #409EFF);\r\n  border-radius: 3px;\r\n}\r\n\r\n/* 表单样式优化 */\r\n.el-form {\r\n  padding: 0 30px 20px;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 25px;\r\n}\r\n\r\n.el-input {\r\n  border-radius: 8px;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.el-input:focus-within .el-input__inner {\r\n  box-shadow: 0 0 0 2px rgba(22, 93, 255, 0.2);\r\n  border-color: #165DFF;\r\n}\r\n\r\n.el-input__prefix {\r\n  color: #8c939d;\r\n}\r\n\r\n/* 按钮样式优化 */\r\n.login-button-container {\r\n  text-align: center;\r\n  padding: 0 30px 30px;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  height: 50px;\r\n  border-radius: 8px;\r\n  font-size: 16px;\r\n  font-weight: 500;\r\n  transition: all 0.3s ease;\r\n  background-color: #165DFF;\r\n  border-color: #165DFF;\r\n}\r\n\r\n.login-button:hover {\r\n  background-color: #0d47a1;\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 4px 12px rgba(22, 93, 255, 0.3);\r\n}\r\n\r\n/* 响应式设计 */\r\n@media (max-width: 420px) {\r\n  .box-card {\r\n    max-width: 340px;\r\n  }\r\n  \r\n  .el-form, .login-button-container {\r\n    padding: 0 20px 20px;\r\n  }\r\n  \r\n  .login-title {\r\n    font-size: 28px;\r\n    padding: 30px 0 20px;\r\n  }\r\n}\r\n\r\n/* 加载动画（可配合脚本使用） */\r\n.login-loading {\r\n  animation: pulse 1.5s infinite;\r\n}\r\n\r\n@keyframes pulse {\r\n  0% { transform: scale(1); }\r\n  50% { transform: scale(1.05); }\r\n  100% { transform: scale(1); }\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAO;;EAqBTA,KAAK,EAAC;AAAwB;;;;;;;uBArBvCC,mBAAA,CA8BM,OA9BNC,UA8BM,GA7BJC,YAAA,CA4BUC,kBAAA;IA5BDJ,KAAK,EAAC,UAAU;IAACK,MAAM,EAAC;;IAFrCC,OAAA,EAAAC,QAAA,CAGM,MAGM,C,0BAHNC,mBAAA,CAGM;MAFJR,KAAK,EAAC;IAAa,GACpB,QACD,sBACAG,YAAA,CAcUM,kBAAA;MAbRC,GAAG,EAAC,WAAW;MACdC,KAAK,EAAEC,MAAA,CAAAC,SAAS;MACjBC,IAAI,EAAC,OAAO;MACZ,gBAAc,EAAC,OAAO;MACtB,aAAW,EAAC,MAAM;MAClBC,KAAsC,EAAtC;QAAA;QAAA;MAAA;;MAbRT,OAAA,EAAAC,QAAA,CAeQ,MAEe,CAF