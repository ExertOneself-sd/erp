{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { onMounted, reactive, ref } from 'vue';\nexport default {\n  __name: 'BuyListManager',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const buyList = ref([]);\n    const total = ref(0);\n    const currentPage = ref(1);\n    function queryBuyList(pageNum) {\n      currentPage.value = pageNum;\n      axios.get('http://localhost:8081/queryBuyList?pageNum=' + pageNum).then(response => {\n        buyList.value = response.data.buyLists;\n        total.value = response.data.total;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    onMounted(function () {\n      queryBuyList(1);\n    });\n    function buyListPageChange(pageNum) {\n      queryBuyList(pageNum);\n    }\n    const buyForm = reactive({\n      productId: '',\n      storeId: '',\n      supplyId: '',\n      placeId: '',\n      itemName: '',\n      storeName: '',\n      supplyName: '',\n      placeName: '',\n      buyUser: '',\n      buyNum: '',\n      factBuyNum: '',\n      phone: ''\n    });\n    const buyListDialog = ref(false);\n    function showBuyListDialog(row) {\n      var productId = row.productId;\n      buyListDialog.value = true;\n      axios.get(\"http://localhost:8081/buyAutoInfo/\" + productId).then(response => {\n        var item = response.data;\n        buyForm.productId = item.id;\n        buyForm.itemName = item.itemName;\n        buyForm.storeId = item.storeId;\n        buyForm.storeName = item.storeName;\n        buyForm.supplyId = item.supplyId;\n        buyForm.supplyName = item.supplyName;\n        buyForm.placeId = item.placeId;\n        buyForm.placeName = item.placeName;\n        buyForm.buyUser = row.buyUser;\n        buyForm.buyNum = row.buyNum;\n        buyForm.factBuyNum = row.factBuyNum;\n        buyForm.phone = row.phone;\n        //表单中封装采购单id\n        buyForm.buyId = row.buyId;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function updateBuyOrder() {\n      axios.post(\"http://localhost:8081/updateBuyList\", buyForm).then(response => {\n        if (response.data.code == 200) {\n          buyListDialog.value = false;\n          currentPage.value = 1;\n          queryBuyList(1);\n        }\n        ElMessage(response.data.msg);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const __returned__ = {\n      buyList,\n      total,\n      currentPage,\n      queryBuyList,\n      buyListPageChange,\n      buyForm,\n      buyListDialog,\n      showBuyListDialog,\n      updateBuyOrder,\n      get axios() {\n        return axios;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      onMounted,\n      reactive,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","ElMessage","onMounted","reactive","ref","buyList","total","currentPage","queryBuyList","pageNum","value","get","then","response","data","buyLists","catch","error","console","log","buyListPageChange","buyForm","productId","storeId","supplyId","placeId","itemName","storeName","supplyName","placeName","buyUser","buyNum","factBuyNum","phone","buyListDialog","showBuyListDialog","row","item","id","buyId","updateBuyOrder","post","code","msg"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/BuyListManager.vue"],"sourcesContent":["<template>\r\n  <h2>采购单列表</h2>\r\n  <el-table :data=\"buyList\"  style=\"width: 100%\">\r\n    <el-table-column prop=\"storeName\" label=\"仓库名称\"  />\r\n    <el-table-column prop=\"itemName\" label=\"商品名称\" />\r\n    <el-table-column prop=\"buyNum\" label=\"预计采购数量\"  />\r\n    <el-table-column prop=\"factBuyNum\" label=\"实际采购数量\" />\r\n    <el-table-column prop=\"buyUser\" label=\"采购人\" />\r\n    <el-table-column prop=\"buyTime\" label=\"采购时间\" />\r\n    <el-table-column prop=\"phone\" label=\"采购人电话\"  />\r\n    <el-table-column prop=\"isIn\" label=\"状态\" width=\"100\">\r\n        <template #default=\"scope\" >\r\n            <span v-if=\"scope.row.isIn==0\" style=\"color:red\">未入库</span>\r\n            <span v-else style=\"color:green\">已入库</span>\r\n        </template>\r\n    </el-table-column>\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\r\n        <template #default=\"scope\" >\r\n            <el-button link type=\"primary\" v-if=\"scope.row.factBuyNum==0\" size=\"small\" @click=\"showBuyListDialog(scope.row)\">修改</el-button>\r\n            <el-button link type=\"primary\" v-if=\"scope.row.factBuyNum==0\" size=\"small\">删除</el-button>\r\n            <el-button link type=\"primary\" v-if=\"scope.row.isIn==0\" size=\"small\">生成订单</el-button>\r\n        </template>\r\n    </el-table-column>\r\n  </el-table>\r\n  <hr/>\r\n  <el-pagination\r\n      small\r\n      background\r\n      :page-size=\"6\"\r\n      :pager-count=\"10\"\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\"\r\n      class=\"mt-4\" @current-change=\"buyListPageChange\"/>\r\n\r\n    <el-dialog\r\n      v-model=\"buyListDialog\"\r\n      width=\"40%\">\r\n    <h2>商品采购</h2>\r\n    <el-form :model=\"buyForm\" label-width=\"120px\">\r\n        <el-form-item label=\"商品名称：\">\r\n          {{buyForm.itemName}}\r\n        </el-form-item>\r\n        <el-form-item label=\"仓库：\">\r\n          {{buyForm.storeName}}\r\n        </el-form-item>\r\n        <el-form-item label=\"供应商：\">\r\n          {{buyForm.supplyName}}\r\n        </el-form-item>\r\n        <el-form-item label=\"产地：\">\r\n          {{buyForm.placeName}}\r\n        </el-form-item>\r\n        <el-form-item label=\"预计采购量：\" required>\r\n          <el-input v-model=\"buyForm.buyNum\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"实际采购量：\" required>\r\n          <el-input v-model=\"buyForm.factBuyNum\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"采购人：\" required>\r\n          <el-input v-model=\"buyForm.buyUser\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"采购人电话：\">\r\n          <el-input v-model=\"buyForm.phone\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"updateBuyOrder\">保存</el-button>\r\n          <el-button type=\"primary\" >清空</el-button>\r\n        </el-form-item>\r\n    </el-form>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\nimport { onMounted,reactive, ref } from 'vue';\r\n\r\n\r\nconst buyList=ref([])\r\nconst total=ref(0)\r\nconst currentPage = ref(1); \r\n\r\n\r\nfunction queryBuyList(pageNum){\r\n    currentPage.value = pageNum; \r\n    axios.get('http://localhost:8081/queryBuyList?pageNum='+pageNum)\r\n    .then((response)=>{\r\n        buyList.value=response.data.buyLists\r\n        total.value=response.data.total\r\n    })\r\n    .catch((error)=>{\r\n        console.log(error)\r\n    })\r\n}\r\n\r\nonMounted(function(){\r\n    queryBuyList(1)\r\n})\r\n\r\nfunction buyListPageChange(pageNum){\r\n  queryBuyList(pageNum)\r\n}\r\n\r\nconst buyForm=reactive({\r\n  productId:'',\r\n  storeId:'',\r\n  supplyId:'',\r\n  placeId:'',\r\n  itemName:'',\r\n  storeName:'',\r\n  supplyName:'',\r\n  placeName:'',\r\n  buyUser:'',\r\n  buyNum:'',\r\n  factBuyNum:'',\r\n  phone:''\r\n})\r\n\r\nconst buyListDialog=ref(false)\r\n\r\nfunction showBuyListDialog(row){\r\n    var productId=row.productId\r\n    buyListDialog.value=true\r\n    axios.get(\"http://localhost:8081/buyAutoInfo/\"+productId)\r\n  .then((response)=>{\r\n    var item=response.data\r\n    buyForm.productId=item.id\r\n    buyForm.itemName=item.itemName\r\n    buyForm.storeId=item.storeId\r\n    buyForm.storeName=item.storeName\r\n    buyForm.supplyId=item.supplyId\r\n    buyForm.supplyName=item.supplyName\r\n    buyForm.placeId=item.placeId\r\n    buyForm.placeName=item.placeName\r\n    buyForm.buyUser=row.buyUser\r\n    buyForm.buyNum=row.buyNum\r\n    buyForm.factBuyNum=row.factBuyNum\r\n    buyForm.phone=row.phone\r\n    //表单中封装采购单id\r\n    buyForm.buyId=row.buyId\r\n\r\n  })\r\n  .catch((error)=>{\r\n    console.log(error);\r\n  })\r\n}\r\n\r\nfunction updateBuyOrder(){\r\n    axios.post(\"http://localhost:8081/updateBuyList\",buyForm)\r\n    .then((response)=>{\r\n        if(response.data.code==200){\r\n             buyListDialog.value=false\r\n             currentPage.value = 1; \r\n             queryBuyList(1)\r\n        }\r\n        ElMessage(response.data.msg)\r\n    })\r\n    .catch((error)=>{\r\n        console.log(error)\r\n    })\r\n\r\n}\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAwEA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,cAAc;AACxC,SAASC,SAAS,EAACC,QAAQ,EAAEC,GAAG,QAAQ,KAAK;;;;;;;IAG7C,MAAMC,OAAO,GAACD,GAAG,CAAC,EAAE,CAAC;IACrB,MAAME,KAAK,GAACF,GAAG,CAAC,CAAC,CAAC;IAClB,MAAMG,WAAW,GAAGH,GAAG,CAAC,CAAC,CAAC;IAG1B,SAASI,YAAYA,CAACC,OAAO,EAAC;MAC1BF,WAAW,CAACG,KAAK,GAAGD,OAAO;MAC3BT,KAAK,CAACW,GAAG,CAAC,6CAA6C,GAACF,OAAO,CAAC,CAC/DG,IAAI,CAAEC,QAAQ,IAAG;QACdR,OAAO,CAACK,KAAK,GAACG,QAAQ,CAACC,IAAI,CAACC,QAAQ;QACpCT,KAAK,CAACI,KAAK,GAACG,QAAQ,CAACC,IAAI,CAACR,KAAK;MACnC,CAAC,CAAC,CACDU,KAAK,CAAEC,KAAK,IAAG;QACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;IAEAf,SAAS,CAAC,YAAU;MAChBM,YAAY,CAAC,CAAC,CAAC;IACnB,CAAC,CAAC;IAEF,SAASY,iBAAiBA,CAACX,OAAO,EAAC;MACjCD,YAAY,CAACC,OAAO,CAAC;IACvB;IAEA,MAAMY,OAAO,GAAClB,QAAQ,CAAC;MACrBmB,SAAS,EAAC,EAAE;MACZC,OAAO,EAAC,EAAE;MACVC,QAAQ,EAAC,EAAE;MACXC,OAAO,EAAC,EAAE;MACVC,QAAQ,EAAC,EAAE;MACXC,SAAS,EAAC,EAAE;MACZC,UAAU,EAAC,EAAE;MACbC,SAAS,EAAC,EAAE;MACZC,OAAO,EAAC,EAAE;MACVC,MAAM,EAAC,EAAE;MACTC,UAAU,EAAC,EAAE;MACbC,KAAK,EAAC;IACR,CAAC,CAAC;IAEF,MAAMC,aAAa,GAAC9B,GAAG,CAAC,KAAK,CAAC;IAE9B,SAAS+B,iBAAiBA,CAACC,GAAG,EAAC;MAC3B,IAAId,SAAS,GAACc,GAAG,