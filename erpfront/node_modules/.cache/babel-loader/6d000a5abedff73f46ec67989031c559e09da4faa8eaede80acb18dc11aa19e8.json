{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage, ElLoading } from 'element-plus';\nimport { onMounted, reactive, ref } from 'vue';\nexport default {\n  __name: 'BuyListManager',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const buyList = ref([]);\n    const total = ref(0);\n    const currentPage = ref(1);\n    const pageSize = ref(6);\n    const loading = ref(false);\n    function queryBuyList(pageNum, pageSize) {\n      loading.value = true;\n      axios.get(`http://localhost:8081/queryBuyList?pageNum=${pageNum}&pageSize=${pageSize}`).then(response => {\n        buyList.value = response.data.buyLists;\n        total.value = response.data.total;\n      }).catch(error => {\n        ElMessage.error('数据加载失败，请稍后再试');\n        console.log(error);\n      }).finally(() => {\n        loading.value = false;\n      });\n    }\n    onMounted(() => {\n      queryBuyList(currentPage.value, pageSize.value);\n    });\n    function handlePageChange(pageNum) {\n      currentPage.value = pageNum;\n      queryBuyList(pageNum, pageSize.value);\n    }\n    function handleSizeChange(pageSize) {\n      pageSize.value = pageSize;\n      currentPage.value = 1;\n      queryBuyList(currentPage.value, pageSize.value);\n    }\n    const buyForm = reactive({\n      productId: '',\n      storeId: '',\n      supplyId: '',\n      placeId: '',\n      itemName: '',\n      storeName: '',\n      supplyName: '',\n      placeName: '',\n      buyUser: '',\n      buyNum: '',\n      factBuyNum: '',\n      phone: ''\n    });\n    const buyListDialog = ref(false);\n    function showBuyListDialog(row) {\n      var productId = row.productId;\n      buyListDialog.value = true;\n      axios.get(\"http://localhost:8081/buyAutoInfo/\" + productId).then(response => {\n        var item = response.data;\n        buyForm.productId = item.id;\n        buyForm.itemName = item.itemName;\n        buyForm.storeId = item.storeId;\n        buyForm.storeName = item.storeName;\n        buyForm.supplyId = item.supplyId;\n        buyForm.supplyName = item.supplyName;\n        buyForm.placeId = item.placeId;\n        buyForm.placeName = item.placeName;\n        buyForm.buyUser = row.buyUser;\n        buyForm.buyNum = row.buyNum;\n        buyForm.factBuyNum = row.factBuyNum;\n        buyForm.phone = row.phone;\n        //表单中封装采购单id\n        buyForm.buyId = row.buyId;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function updateBuyOrder() {\n      axios.post(\"http://localhost:8081/updateBuyList\", buyForm).then(response => {\n        if (response.data.code == 200) {\n          buyListDialog.value = false;\n          queryBuyList(1);\n        }\n        ElMessage(response.data.msg);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const __returned__ = {\n      buyList,\n      total,\n      currentPage,\n      pageSize,\n      loading,\n      queryBuyList,\n      handlePageChange,\n      handleSizeChange,\n      buyForm,\n      buyListDialog,\n      showBuyListDialog,\n      updateBuyOrder,\n      get axios() {\n        return axios;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get ElLoading() {\n        return ElLoading;\n      },\n      onMounted,\n      reactive,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","ElMessage","ElLoading","onMounted","reactive","ref","buyList","total","currentPage","pageSize","loading","queryBuyList","pageNum","value","get","then","response","data","buyLists","catch","error","console","log","finally","handlePageChange","handleSizeChange","buyForm","productId","storeId","supplyId","placeId","itemName","storeName","supplyName","placeName","buyUser","buyNum","factBuyNum","phone","buyListDialog","showBuyListDialog","row","item","id","buyId","updateBuyOrder","post","code","msg"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/BuyListManager.vue"],"sourcesContent":["<template>\r\n  <h2>采购单列表</h2>\r\n  <el-table :data=\"buyList\"  style=\"width: 100%\">\r\n    <el-table-column prop=\"storeName\" label=\"仓库名称\"  />\r\n    <el-table-column prop=\"itemName\" label=\"商品名称\" />\r\n    <el-table-column prop=\"buyNum\" label=\"预计采购数量\"  />\r\n    <el-table-column prop=\"factBuyNum\" label=\"实际采购数量\" />\r\n    <el-table-column prop=\"buyUser\" label=\"采购人\" />\r\n    <el-table-column prop=\"buyTime\" label=\"采购时间\" />\r\n    <el-table-column prop=\"phone\" label=\"采购人电话\"  />\r\n    <el-table-column prop=\"isIn\" label=\"状态\" width=\"100\">\r\n        <template #default=\"scope\" >\r\n            <span v-if=\"scope.row.isIn==0\" style=\"color:red\">未入库</span>\r\n            <span v-else style=\"color:green\">已入库</span>\r\n        </template>\r\n    </el-table-column>\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\r\n        <template #default=\"scope\" >\r\n            <el-button link type=\"primary\" v-if=\"scope.row.factBuyNum==0\" size=\"small\" @click=\"showBuyListDialog(scope.row)\">修改</el-button>\r\n            <el-button link type=\"primary\" v-if=\"scope.row.factBuyNum==0\" size=\"small\">删除</el-button>\r\n            <el-button link type=\"primary\" v-if=\"scope.row.isIn==0\" size=\"small\">生成订单</el-button>\r\n        </template>\r\n    </el-table-column>\r\n  </el-table>\r\n  <hr/>\r\n  <el-pagination\r\n      small\r\n      background\r\n      :page-size=\"6\"\r\n      :pager-count=\"10\"\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\"\r\n      class=\"mt-4\"\r\n      :current-page=\"currentPage\"\r\n      @current-change=\"handlePageChange\"\r\n      @size-change=\"handleSizeChange\"/>\r\n\r\n  <el-dialog\r\n      v-model=\"buyListDialog\"\r\n      width=\"40%\">\r\n    <!-- 对话框内容保持不变 -->\r\n  </el-dialog>\r\n  <el-loading v-if=\"loading\" :lock=\"true\" text=\"加载中...\"></el-loading>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from 'axios';\r\nimport { ElMessage, ElLoading } from 'element-plus';\r\nimport { onMounted, reactive, ref } from 'vue';\r\n\r\nconst buyList = ref([]);\r\nconst total = ref(0);\r\nconst currentPage = ref(1);\r\nconst pageSize = ref(6);\r\nconst loading = ref(false);\r\n\r\nfunction queryBuyList(pageNum, pageSize) {\r\n    loading.value = true;\r\n    axios.get(`http://localhost:8081/queryBuyList?pageNum=${pageNum}&pageSize=${pageSize}`)\r\n      .then((response) => {\r\n          buyList.value = response.data.buyLists;\r\n          total.value = response.data.total;\r\n      })\r\n      .catch((error) => {\r\n          ElMessage.error('数据加载失败，请稍后再试');\r\n          console.log(error);\r\n      })\r\n      .finally(() => {\r\n          loading.value = false;\r\n      });\r\n}\r\n\r\nonMounted(() => {\r\n    queryBuyList(currentPage.value, pageSize.value);\r\n});\r\n\r\nfunction handlePageChange(pageNum) {\r\n    currentPage.value = pageNum;\r\n    queryBuyList(pageNum, pageSize.value);\r\n}\r\n\r\nfunction handleSizeChange(pageSize) {\r\n    pageSize.value = pageSize;\r\n    currentPage.value = 1;\r\n    queryBuyList(currentPage.value, pageSize.value);\r\n}\r\n\r\nconst buyForm=reactive({\r\n  productId:'',\r\n  storeId:'',\r\n  supplyId:'',\r\n  placeId:'',\r\n  itemName:'',\r\n  storeName:'',\r\n  supplyName:'',\r\n  placeName:'',\r\n  buyUser:'',\r\n  buyNum:'',\r\n  factBuyNum:'',\r\n  phone:''\r\n})\r\n\r\nconst buyListDialog=ref(false)\r\n\r\nfunction showBuyListDialog(row){\r\n    var productId=row.productId\r\n    buyListDialog.value=true\r\n    axios.get(\"http://localhost:8081/buyAutoInfo/\"+productId)\r\n  .then((response)=>{\r\n    var item=response.data\r\n    buyForm.productId=item.id\r\n    buyForm.itemName=item.itemName\r\n    buyForm.storeId=item.storeId\r\n    buyForm.storeName=item.storeName\r\n    buyForm.supplyId=item.supplyId\r\n    buyForm.supplyName=item.supplyName\r\n    buyForm.placeId=item.placeId\r\n    buyForm.placeName=item.placeName\r\n    buyForm.buyUser=row.buyUser\r\n    buyForm.buyNum=row.buyNum\r\n    buyForm.factBuyNum=row.factBuyNum\r\n    buyForm.phone=row.phone\r\n    //表单中封装采购单id\r\n    buyForm.buyId=row.buyId\r\n\r\n  })\r\n  .catch((error)=>{\r\n    console.log(error);\r\n  })\r\n}\r\n\r\nfunction updateBuyOrder(){\r\n    axios.post(\"http://localhost:8081/updateBuyList\",buyForm)\r\n    .then((response)=>{\r\n        if(response.data.code==200){\r\n             buyListDialog.value=false\r\n             queryBuyList(1)\r\n        }\r\n        ElMessage(response.data.msg)\r\n    })\r\n    .catch((error)=>{\r\n        console.log(error)\r\n    })\r\n\r\n}\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AA8CA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,EAAEC,SAAS,QAAQ,cAAc;AACnD,SAASC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAQ,KAAK;;;;;;;IAE9C,MAAMC,OAAO,GAAGD,GAAG,CAAC,EAAE,CAAC;IACvB,MAAME,KAAK,GAAGF,GAAG,CAAC,CAAC,CAAC;IACpB,MAAMG,WAAW,GAAGH,GAAG,CAAC,CAAC,CAAC;IAC1B,MAAMI,QAAQ,GAAGJ,GAAG,CAAC,CAAC,CAAC;IACvB,MAAMK,OAAO,GAAGL,GAAG,CAAC,KAAK,CAAC;IAE1B,SAASM,YAAYA,CAACC,OAAO,EAAEH,QAAQ,EAAE;MACrCC,OAAO,CAACG,KAAK,GAAG,IAAI;MACpBb,KAAK,CAACc,GAAG,CAAC,8CAA8CF,OAAO,aAAaH,QAAQ,EAAE,CAAC,CACpFM,IAAI,CAAEC,QAAQ,IAAK;QAChBV,OAAO,CAACO,KAAK,GAAGG,QAAQ,CAACC,IAAI,CAACC,QAAQ;QACtCX,KAAK,CAACM,KAAK,GAAGG,QAAQ,CAACC,IAAI,CAACV,KAAK;MACrC,CAAC,CAAC,CACDY,KAAK,CAAEC,KAAK,IAAK;QACdnB,SAAS,CAACmB,KAAK,CAAC,cAAc,CAAC;QAC/BC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC,CAAC,CACDG,OAAO,CAAC,MAAM;QACXb,OAAO,CAACG,KAAK,GAAG,KAAK;MACzB,CAAC,CAAC;IACR;IAEAV,SAAS,CAAC,MAAM;MACZQ,YAAY,CAACH,WAAW,CAACK,KAAK,EAAEJ,QAAQ,CAACI,KAAK,CAAC;IACnD,CAAC,CAAC;IAEF,SAASW,gBAAgBA,CAACZ,OAAO,EAAE;MAC/BJ,WAAW,CAACK,KAAK,GAAGD,OAAO;MAC3BD,YAAY,CAACC,OAAO,EAAEH,QAAQ,CAACI,KAAK,CAAC;IACzC;IAEA,SAASY,gBAAgBA,CAAChB,QAAQ,EAAE;MAChCA,QAAQ,CAACI,KAAK,GAAGJ,QAAQ;MACzBD,WAAW,CAACK,KAAK,GAAG,CAAC;MACrBF,YAAY,CAACH,WAAW,CAACK,KAAK,EAAEJ,QAAQ,CAACI,KAAK,CAAC;IACnD;IAEA,MAAMa,OAAO,GAACtB,Q