h: 80%\" readonly=\"readonly\"/>\r\n    </el-form-item>\r\n\r\n\r\n    <el-form-item>\r\n        <el-button type=\"primary\" @click=\"subUpdateCustForm\">保存</el-button>\r\n        <el-button>取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n\r\n  </el-dialog>\r\n\r\n</template>\r\n\r\n\r\n<script setup>\r\n\r\nimport {onMounted, reactive, ref} from \"vue\";\r\nimport axios from \"axios\";\r\n//声明销售机会列表集合\r\nconst sellJhList=ref([]);\r\n//声明total\r\nconst total=ref(0);\r\n\r\n//定义函数发生ajax请求加载列表\r\nfunction querySellJhList(pageNum){\r\n  axios.get(\"http://localhost:8081/sellJhList?pageNum=\"+pageNum)\r\n  .then((response)=>{\r\n    sellJhList.value=response.data.sellJhList;\r\n    total.value=response.data.total;\r\n  })\r\n  .catch((error)=>{\r\n    console.log(error);\r\n  });\r\n}\r\n\r\n//页面加载发送ajax请求\r\nonMounted(function(){\r\n  querySellJhList(1);\r\n});\r\n//定义分页按钮的回调函数\r\nfunction selljHPageChange(pageNum){\r\n  querySellJhList(pageNum);\r\n}\r\n//定义函数，将数字表示的渠道转化为渠道名字\r\nfunction getChannelName(channelId){\r\n  switch (channelId){\r\n    case 0:\r\n      return \"自媒体\";\r\n    case 1:\r\n      return \"网络推广\";\r\n    case 2:\r\n       return \"老客户介绍\"\r\n    case 3:\r\n      return \"陌拜\"\r\n    case 4:\r\n      return \"二次客户\"\r\n\r\n  }\r\n}\r\n\r\n////////////////////////////////////////////////////////////////////////\r\nconst  dialogSellVisible=ref(false);\r\n//声明表单对象数据\r\nconst sellJhForm=reactive({\r\n  custid:'',\r\n  channelId:'',\r\n  money:0,\r\n  nowStep:'',\r\n  empId:''\r\n});\r\n//声明客户列表\r\nconst custList=ref([]);\r\n//定义函数打开数据回显的对话框\r\nfunction openSellDialog(row){\r\n\r\n  dialogSellVisible.value=true;\r\n  //打开对话框发送ajax请求，加载所有客户信息\r\n  axios.get(\"http://localhost:8081/listAllCust\")\r\n      .then((response)=>{\r\n        custList.value=response.data;\r\n        //下拉列表框渲染结束后，再实现回显\r\n        sellJhForm.custid=row.custid;\r\n        sellJhForm.channelId=row.channelId;\r\n        sellJhForm.money=row.money;\r\n        sellJhForm.empId=row.empId;\r\n        sellJhForm.nowStep=row.nowStep;\r\n        sellJhForm.id=row.id;\r\n        console.log(sellJhForm);\r\n      })\r\n      .catch((error)=>{\r\n        console.log(error);\r\n      });\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAsGA,SAAQA,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAK;AAC5C,OAAOC,KAAK,MAAM,OAAO;AACzB;;;;;;;;IACA,MAAMC,UAAU,GAACF,GAAG,CAAC,EAAE,CAAC;IACxB;IACA,MAAMG,KAAK,GAACH,GAAG,CAAC,CAAC,CAAC;;IAElB;IACA,SAASI,eAAeA,CAACC,OAAO,EAAC;MAC/BJ,KAAK,CAACK,GAAG,CAAC,2CAA2C,GAACD,OAAO,CAAC,CAC7DE,IAAI,CAAEC,QAAQ,IAAG;QAChBN,UAAU,CAACO,KAAK,GAACD,QAAQ,CAACE,IAAI,CAACR,UAAU;QACzCC,KAAK,CAACM,KAAK,GAACD,QAAQ,CAACE,IAAI,CAACP,KAAK;MACjC,CAAC,CAAC,CACDQ,KAAK,CAAEC,KAAK,IAAG;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;;IAEA;IACAd,SAAS,CAAC,YAAU;MAClBM,eAAe,CAAC,CAAC,CAAC;IACpB,CAAC,CAAC;IACF;IACA,SAASW,gBAAgBA,CAACV,OAAO,EAAC;MAChCD,eAAe,CAACC,OAAO,CAAC;IAC1B;IACA;IACA,SAASW,cAAcA,CAACC,SAAS,EAAC;MAChC,QAAQA,SAAS;QACf,KAAK,CAAC;UACJ,OAAO,KAAK;QACd,KAAK,CAAC;UACJ,OAAO,MAAM;QACf,KAAK,CAAC;UACH,OAAO,OAAO;QACjB,KAAK,CAAC;UACJ,OAAO,IAAI;QACb,KAAK,CAAC;UACJ,OAAO,MAAM;MAEjB;IACF;;IAEA;IACA,MAAOC,iBAAiB,GAAClB,GAAG,CAAC,KAAK,CAAC;IACnC;IACA,MAAMmB,UAAU,GAACpB,QAAQ,CAAC;MACxBqB,MAAM,EAAC,EAAE;MACTH,SAAS,EAAC,EAAE;MACZI,KAAK,EAAC,CAAC;MACPC,OAAO,EAAC,EAAE;MACVC,KAAK,EAAC;IACR,CAAC,CAAC;IACF;IACA,MAAMC,QAAQ,GAACxB,GAAG,CAAC,EAAE,CAAC;IACtB;IACA,SAASyB,cAAcA,CAACC,GAAG,EAAC;MAE1BR,iBAAiB,CAACT,KAAK,GAAC,IAAI;MAC5B;MACAR,KAAK,CAACK,GAAG,CAAC,mCAAmC,CAAC,CACzCC,IAAI,CAAEC,QAAQ,IAAG;QAChBgB,QAAQ,CAACf,KAAK,GAACD,QAAQ,CAACE,IAAI;QAC5B;QACAS,UAAU,CAACC,MAAM,GAACM,GAAG,CAACN,MAAM;QAC5BD,UAAU,CAACF,SAAS,GAACS,GAAG,CAACT,SAAS;QAClCE,UAAU,CAACE,KAAK,GAACK,GAAG,CAACL,KAAK;QAC1BF,UAAU,CAACI,KAAK,GAACG,GAAG,CAACH,KAAK;QAC1BJ,UAAU,CAACG,OAAO,GAACI,GAAG,CAACJ,OAAO;QAC9BH,UAAU,CAACQ,EAAE,GAACD,GAAG,CAACC,EAAE;QACpBd,OAAO,CAACC,GAAG,CAACK,UAAU,CAAC;MACzB,CAAC,CAAC,CACDR,KAAK,CAAEC,KAAK,IAAG;QACdC,OAAO,CAACC,GAAG,CAACF,KAA