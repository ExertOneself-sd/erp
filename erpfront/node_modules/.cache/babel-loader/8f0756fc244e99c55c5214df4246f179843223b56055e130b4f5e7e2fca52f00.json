     console.log(sellJhForm);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const __returned__ = {\n      sellJhList,\n      total,\n      querySellJhList,\n      selljHPageChange,\n      getChannelName,\n      dialogSellVisible,\n      sellJhForm,\n      custList,\n      openSellDialog,\n      onMounted,\n      reactive,\n      ref,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","reactive","ref","axios","sellJhList","total","querySellJhList","pageNum","get","then","response","value","data","catch","error","console","log","selljHPageChange","getChannelName","channelId","dialogSellVisible","sellJhForm","custid","money","nowStep","empId","custList","openSellDialog","row","id"],"sources":["D:/Program/Web QP/vue/erpfront/src/views/ListSellJh.vue"],"sourcesContent":["<template>\r\n  <h2>客户销售过程列表</h2>\r\n\r\n  <el-table :data=\"sellJhList\" stripe style=\"width: 100%\">\r\n    <el-table-column prop=\"id\" label=\"编号\" width=\"180\"/>\r\n    <el-table-column prop=\"custName\" label=\"客户\" width=\"180\"/>\r\n    <el-table-column prop=\"channelId\" label=\"销售渠道\">\r\n      <template #default=\"scope\">\r\n        {{getChannelName(scope.row.channelId)}}\r\n      </template>\r\n    </el-table-column>\r\n    <el-table-column prop=\"money\" label=\"销售金额\"/>\r\n    <el-table-column prop=\"nowStep\" label=\"销售阶段\"/>\r\n    <el-table-column prop=\"empId\" label=\"业务员\"/>\r\n\r\n\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"120\">\r\n      <template #default=\"scope\">\r\n        <el-button link type=\"primary\" size=\"small\" >删除\r\n        </el-button>\r\n        <el-button link type=\"primary\" size=\"small\"\r\n                   @click=\"openSellDialog(scope.row)\">修改\r\n        </el-button>\r\n      </template>\r\n    </el-tab