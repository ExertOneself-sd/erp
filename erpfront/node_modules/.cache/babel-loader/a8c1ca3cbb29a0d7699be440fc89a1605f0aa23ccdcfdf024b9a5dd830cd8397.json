{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { Plus, Delete, ZoomIn } from '@element-plus/icons-vue';\nimport axios from 'axios';\n\n// 对话框状态\n\nexport default {\n  __name: 'ItemManager',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const dialogItemVisible = ref(false);\n    const dialogVisible = ref(false);\n    const dialogImageUrl = ref(null);\n\n    // 商品表单数据\n    const itemForm = reactive({\n      id: 0,\n      name: '',\n      code: '',\n      category: '',\n      stock: 0,\n      brand: '',\n      warehouse: '',\n      supplier: '',\n      origin: '',\n      unit: '',\n      purchasePrice: '',\n      sellingPrice: '',\n      vipPrice: '',\n      description: '',\n      productionDate: null,\n      expiryDate: null,\n      imageUrl: ''\n    });\n\n    // 下拉列表数据\n    const supplyList = ref([]);\n    const placeList = ref([]);\n    const storeList = ref([]);\n    const brandList = ref([]);\n    const unitList = ref([]);\n\n    // 生命周期钩子\n    onMounted(() => {\n      // 页面加载时初始化数据\n      loadInitialData();\n    });\n\n    // 打开添加商品对话框\n    function openItemDialog() {\n      // 重置表单\n      resetForm();\n\n      // 显示对话框\n      dialogItemVisible.value = true;\n\n      // 加载下拉列表数据\n      loadDropdownData();\n    }\n\n    // 加载初始数据\n    function loadInitialData() {\n      // 这里可以添加页面初始化需要的数据\n    }\n\n    // 加载下拉列表数据\n    function loadDropdownData() {\n      loadSupplyList();\n      loadPlaceList();\n      loadStoreList();\n      loadBrandList();\n      loadUnitList();\n    }\n\n    // 重置表单\n    function resetForm() {\n      itemForm.id = 0;\n      itemForm.name = '';\n      itemForm.code = '';\n      itemForm.category = '';\n      itemForm.stock = 0;\n      itemForm.brand = '';\n      itemForm.warehouse = '';\n      itemForm.supplier = '';\n      itemForm.origin = '';\n      itemForm.unit = '';\n      itemForm.purchasePrice = '';\n      itemForm.sellingPrice = '';\n      itemForm.vipPrice = '';\n      itemForm.description = '';\n      itemForm.productionDate = null;\n      itemForm.expiryDate = null;\n      itemForm.imageUrl = '';\n    }\n\n    // 图片预览\n    function handlePictureCardPreview(file) {\n      dialogImageUrl.value = file.url || file.response?.url || file.response?.data?.url;\n      dialogVisible.value = true;\n    }\n\n    // 处理图片上传成功\n    function handleAvatarSuccess(response, file, fileList) {\n      if (response && response.code === 200) {\n        itemForm.imageUrl = response.data.url || response.url;\n        ElMessage.success('图片上传成功');\n      } else {\n        ElMessage.error('图片上传失败');\n      }\n    }\n\n    // 处理图片移除\n    function handleRemove(file, fileList) {\n      itemForm.imageUrl = '';\n    }\n\n    // 处理上传错误\n    function handleUploadError(err, file, fileList) {\n      ElMessage.error('图片上传失败，请重试');\n      console.error('Upload error:', err);\n    }\n\n    // 关闭对话框前的处理\n    function handleClose(done) {\n      // 检查表单是否有修改\n      // 如果有修改，可以提示用户确认是否放弃\n      done();\n    }\n\n    // 保存商品信息\n    function saveItem() {\n      // 这里添加表单验证逻辑\n      console.log('保存商品信息:', itemForm);\n\n      // 模拟API调用\n      setTimeout(() => {\n        dialogItemVisible.value = false;\n        ElMessage.success('商品信息保存成功');\n      }, 500);\n    }\n\n    // 加载供应商列表\n    function loadSupplyList() {\n      axios.get(\"http://localhost:8081/supplyList\").then(response => {\n        supplyList.value = response.data || [];\n      }).catch(error => {\n        console.log(error);\n        ElMessage.error('加载供应商列表失败');\n      });\n    }\n\n    // 加载产地列表\n    function loadPlaceList() {\n      axios.get(\"http://localhost:8081/placeList\").then(response => {\n        placeList.value = response.data || [];\n      }).catch(error => {\n        console.log(error);\n        ElMessage.error('加载产地列表失败');\n      });\n    }\n\n    // 加载仓库列表\n    function loadStoreList() {\n      axios.get(\"http://localhost:8081/storeList\").then(response => {\n        storeList.value = response.data || [];\n      }).catch(error => {\n        console.log(error);\n        ElMessage.error('加载仓库列表失败');\n      });\n    }\n\n    // 加载品牌列表\n    function loadBrandList() {\n      axios.get(\"http://localhost:8081/brandList\").then(response => {\n        brandList.value = response.data || [];\n      }).catch(error => {\n        console.log(error);\n        ElMessage.error('加载品牌列表失败');\n      });\n    }\n\n    // 加载单位列表\n    function loadUnitList() {\n      axios.get(\"http://localhost:8081/unitList\").then(response => {\n        unitList.value = response.data || [];\n      }).catch(error => {\n        console.log(error);\n        ElMessage.error('加载单位列表失败');\n      });\n    }\n    const __returned__ = {\n      dialogItemVisible,\n      dialogVisible,\n      dialogImageUrl,\n      itemForm,\n      supplyList,\n      placeList,\n      storeList,\n      brandList,\n      unitList,\n      openItemDialog,\n      loadInitialData,\n      loadDropdownData,\n      resetForm,\n      handlePictureCardPreview,\n      handleAvatarSuccess,\n      handleRemove,\n      handleUploadError,\n      handleClose,\n      saveItem,\n      loadSupplyList,\n      loadPlaceList,\n      loadStoreList,\n      loadBrandList,\n      loadUnitList,\n      reactive,\n      ref,\n      onMounted,\n      get Plus() {\n        return Plus;\n      },\n      get Delete() {\n        return Delete;\n      },\n      get ZoomIn() {\n        return ZoomIn;\n      },\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","ref","onMounted","Plus","Delete","ZoomIn","axios","dialogItemVisible","dialogVisible","dialogImageUrl","itemForm","id","name","code","category","stock","brand","warehouse","supplier","origin","unit","purchasePrice","sellingPrice","vipPrice","description","productionDate","expiryDate","imageUrl","supplyList","placeList","storeList","brandList","unitList","loadInitialData","openItemDialog","resetForm","value","loadDropdownData","loadSupplyList","loadPlaceList","loadStoreList","loadBrandList","loadUnitList","handlePictureCardPreview","file","url","response","data","handleAvatarSuccess","fileList","ElMessage","success","error","handleRemove","handleUploadError","err","console","handleClose","done","saveItem","log","setTimeout","get","then","catch"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/ItemManager.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-4 max-w-7xl mx-auto\">\r\n    <h2 class=\"text-2xl font-bold mb-6 text-gray-800\">商品信息</h2>\r\n\r\n    <div class=\"flex justify-end mb-4\">\r\n      <el-button type=\"primary\" @click=\"openItemDialog\" class=\"px-6 py-2 bg-blue-600 hover:bg-blue-700 transition-colors duration-200\">\r\n        <el-icon class=\"mr-2\"><Plus /></el-icon>添加商品\r\n      </el-button>\r\n    </div>\r\n\r\n    <el-dialog \r\n      v-model=\"dialogItemVisible\" \r\n      title=\"商品信息管理\"\r\n      :before-close=\"handleClose\"\r\n      class=\"max-w-4xl w-full mx-auto\"\r\n    >\r\n      <template #content>\r\n        <div class=\"space-y-6\">\r\n          <!-- 商品图片上传区域 -->\r\n          <div class=\"bg-gray-50 p-4 rounded-lg\">\r\n            <h3 class=\"text-lg font-semibold mb-3 text-gray-700\">商品图片</h3>\r\n            <el-upload\r\n              action=\"http://localhost:8081/uploadImg\"\r\n              list-type=\"picture-card\"\r\n              :auto-upload=\"true\"\r\n              method=\"post\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n              :on-remove=\"handleRemove\"\r\n              :on-error=\"handleUploadError\"\r\n              class=\"upload-container\"\r\n            >\r\n              <el-icon>\r\n                <Plus />\r\n              </el-icon>\r\n\r\n              <template #file=\"{ file }\">\r\n                <div class=\"relative group\">\r\n                  <img \r\n                    class=\"el-upload-list__item-thumbnail w-full h-full object-cover rounded\" \r\n                    :src=\"file.url\" \r\n                    alt=\"\"\r\n                  />\r\n                  <div \r\n                    class=\"absolute inset-0 bg-black bg-opacity-50 opacity-0 group-hover:opacity-100 transition-opacity duration-200 flex items-center justify-center rounded\"\r\n                  >\r\n                    <span\r\n                      class=\"el-upload-list__item-preview text-white cursor-pointer mr-3\"\r\n                      @click=\"handlePictureCardPreview(file)\"\r\n                    >\r\n                      <el-icon><ZoomIn /></el-icon>\r\n                    </span>\r\n                    <span\r\n                      class=\"el-upload-list__item-delete text-white cursor-pointer\"\r\n                      @click=\"handleRemove(file)\"\r\n                    >\r\n                      <el-icon><Delete /></el-icon>\r\n                    </span>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n            </el-upload>\r\n          </div>\r\n\r\n          <!-- 商品表单区域 -->\r\n          <el-form \r\n            :model=\"itemForm\" \r\n            label-width=\"120px\" \r\n            class=\"space-y-4\"\r\n          >\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"名称\" required>\r\n                  <el-input \r\n                    v-model=\"itemForm.name\" \r\n                    placeholder=\"请输入商品名称\" \r\n                    class=\"w-full\"\r\n                  />\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"编号\" required>\r\n                  <el-input \r\n                    v-model=\"itemForm.code\" \r\n                    placeholder=\"请输入商品编号\" \r\n                    class=\"w-full\"\r\n                  />\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"种类\" required>\r\n                  <el-input \r\n                    v-model=\"itemForm.category\" \r\n                    placeholder=\"选择类型\" \r\n                    class=\"w-full\"\r\n                  />\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"库存\">\r\n                  <el-input-number \r\n                    v-model=\"itemForm.stock\" \r\n                    :min=\"0\" \r\n                    class=\"w-full\"\r\n                  />\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"品牌\" required>\r\n                  <el-select \r\n                    v-model=\"itemForm.brand\" \r\n                    placeholder=\"请选择品牌\" \r\n                    class=\"w-full\"\r\n                  >\r\n                    <el-option\r\n                      v-for=\"item in brandList\"\r\n                      :key=\"item.brandId\"\r\n                      :label=\"item.brandName\"\r\n                      :value=\"item.brandId\"\r\n                    />\r\n                  </el-select>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"仓库\" required>\r\n                  <el-select \r\n                    v-model=\"itemForm.warehouse\" \r\n                    placeholder=\"请选择仓库\" \r\n                    class=\"w-full\"\r\n                  >\r\n                    <el-option\r\n                      v-for=\"item in storeList\"\r\n                      :key=\"item.storeId\"\r\n                      :label=\"item.storeName\"\r\n                      :value=\"item.storeId\"\r\n                    />\r\n                  </el-select>\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"供应商\" required>\r\n                  <el-select \r\n                    v-model=\"itemForm.supplier\" \r\n                    placeholder=\"请选择供应商\" \r\n                    class=\"w-full\"\r\n                  >\r\n                    <el-option\r\n                      v-for=\"item in supplyList\"\r\n                      :key=\"item.supplyId\"\r\n                      :label=\"item.supplyName\"\r\n                      :value=\"item.supplyId\"\r\n                    />\r\n                  </el-select>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"产地\" required>\r\n                  <el-select \r\n                    v-model=\"itemForm.origin\" \r\n                    placeholder=\"请选择产地\" \r\n                    class=\"w-full\"\r\n                  >\r\n                    <el-option\r\n                      v-for=\"item in placeList\"\r\n                      :key=\"item.placeId\"\r\n                      :label=\"item.placeName\"\r\n                      :value=\"item.placeId\"\r\n                    />\r\n                  </el-select>\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n\r\n            <el-form-item label=\"单位\" required>\r\n              <el-select \r\n                v-model=\"itemForm.unit\" \r\n                placeholder=\"请选择单位\" \r\n                class=\"w-full\"\r\n              >\r\n                <el-option\r\n                  v-for=\"item in unitList\"\r\n                  :key=\"item.unitId\"\r\n                  :label=\"item.unitName\"\r\n                  :value=\"item.unitId\"\r\n                />\r\n              </el-select>\r\n            </el-form-item>\r\n\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"进价\">\r\n                  <el-input \r\n                    v-model=\"itemForm.purchasePrice\" \r\n                    placeholder=\"0\" \r\n                    class=\"w-full\"\r\n                  >\r\n                    <template #append>元</template>\r\n                  </el-input>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"售价\">\r\n                  <el-input \r\n                    v-model=\"itemForm.sellingPrice\" \r\n                    placeholder=\"0\" \r\n                    class=\"w-full\"\r\n                  >\r\n                    <template #append>元</template>\r\n                  </el-input>\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n\r\n            <el-form-item label=\"会员价\">\r\n              <el-input \r\n                v-model=\"itemForm.vipPrice\" \r\n                placeholder=\"0\" \r\n                class=\"w-full\"\r\n              >\r\n                <template #append>元</template>\r\n              </el-input>\r\n            </el-form-item>\r\n\r\n            <el-form-item label=\"详细介绍\">\r\n              <el-input \r\n                v-model=\"itemForm.description\" \r\n                type=\"textarea\" \r\n                :rows=\"3\" \r\n                class=\"w-full\"\r\n              />\r\n            </el-form-item>\r\n\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"生产日期\">\r\n                  <el-date-picker \r\n                    v-model=\"itemForm.productionDate\" \r\n                    type=\"date\" \r\n                    placeholder=\"年/月/日\" \r\n                    class=\"w-full\"\r\n                  />\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"保质期\">\r\n                  <el-date-picker \r\n                    v-model=\"itemForm.expiryDate\" \r\n                    type=\"date\" \r\n                    placeholder=\"年/月/日\" \r\n                    class=\"w-full\"\r\n                  />\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n          </el-form>\r\n        </div>\r\n      </template>\r\n\r\n      <template #footer>\r\n        <div class=\"flex justify-end space-x-4\">\r\n          <el-button @click=\"dialogItemVisible = false\" class=\"px-6 py-2 border border-gray-300 rounded hover:bg-gray-50 transition-colors duration-200\">\r\n            取消\r\n          </el-button>\r\n          <el-button \r\n            type=\"primary\" \r\n            @click=\"saveItem\" \r\n            class=\"px-6 py-2 bg-blue-600 text-white rounded hover:bg-blue-700 transition-colors duration-200\"\r\n          >\r\n            保存\r\n          </el-button>\r\n        </div>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <el-dialog \r\n      v-model=\"dialogVisible\" \r\n      title=\"图片预览\"\r\n      class=\"max-w-4xl w-full mx-auto\"\r\n    >\r\n      <template #content>\r\n        <div class=\"flex justify-center\">\r\n          <img \r\n            :src=\"dialogImageUrl\" \r\n            alt=\"Preview Image\" \r\n            class=\"max-w-full max-h-[70vh] object-contain rounded\"\r\n          />\r\n        </div>\r\n      </template>\r\n      <template #footer>\r\n        <el-button @click=\"dialogVisible = false\" class=\"px-6 py-2 border border-gray-300 rounded hover:bg-gray-50 transition-colors duration-200\">\r\n          关闭\r\n        </el-button>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, ref, onMounted } from 'vue'\r\nimport { Plus, Delete, ZoomIn } from '@element-plus/icons-vue'\r\nimport axios from 'axios'\r\n\r\n// 对话框状态\r\nconst dialogItemVisible = ref(false)\r\nconst dialogVisible = ref(false)\r\nconst dialogImageUrl = ref(null)\r\n\r\n// 商品表单数据\r\nconst itemForm = reactive({\r\n  id: 0,\r\n  name: '',\r\n  code: '',\r\n  category: '',\r\n  stock: 0,\r\n  brand: '',\r\n  warehouse: '',\r\n  supplier: '',\r\n  origin: '',\r\n  unit: '',\r\n  purchasePrice: '',\r\n  sellingPrice: '',\r\n  vipPrice: '',\r\n  description: '',\r\n  productionDate: null,\r\n  expiryDate: null,\r\n  imageUrl: ''\r\n})\r\n\r\n// 下拉列表数据\r\nconst supplyList = ref([])\r\nconst placeList = ref([])\r\nconst storeList = ref([])\r\nconst brandList = ref([])\r\nconst unitList = ref([])\r\n\r\n// 生命周期钩子\r\nonMounted(() => {\r\n  // 页面加载时初始化数据\r\n  loadInitialData()\r\n})\r\n\r\n// 打开添加商品对话框\r\nfunction openItemDialog() {\r\n  // 重置表单\r\n  resetForm()\r\n  \r\n  // 显示对话框\r\n  dialogItemVisible.value = true\r\n  \r\n  // 加载下拉列表数据\r\n  loadDropdownData()\r\n}\r\n\r\n// 加载初始数据\r\nfunction loadInitialData() {\r\n  // 这里可以添加页面初始化需要的数据\r\n}\r\n\r\n// 加载下拉列表数据\r\nfunction loadDropdownData() {\r\n  loadSupplyList()\r\n  loadPlaceList()\r\n  loadStoreList()\r\n  loadBrandList()\r\n  loadUnitList()\r\n}\r\n\r\n// 重置表单\r\nfunction resetForm() {\r\n  itemForm.id = 0\r\n  itemForm.name = ''\r\n  itemForm.code = ''\r\n  itemForm.category = ''\r\n  itemForm.stock = 0\r\n  itemForm.brand = ''\r\n  itemForm.warehouse = ''\r\n  itemForm.supplier = ''\r\n  itemForm.origin = ''\r\n  itemForm.unit = ''\r\n  itemForm.purchasePrice = ''\r\n  itemForm.sellingPrice = ''\r\n  itemForm.vipPrice = ''\r\n  itemForm.description = ''\r\n  itemForm.productionDate = null\r\n  itemForm.expiryDate = null\r\n  itemForm.imageUrl = ''\r\n}\r\n\r\n// 图片预览\r\nfunction handlePictureCardPreview(file) {\r\n  dialogImageUrl.value = file.url || file.response?.url || file.response?.data?.url\r\n  dialogVisible.value = true\r\n}\r\n\r\n// 处理图片上传成功\r\nfunction handleAvatarSuccess(response, file, fileList) {\r\n  if (response && response.code === 200) {\r\n    itemForm.imageUrl = response.data.url || response.url\r\n    ElMessage.success('图片上传成功')\r\n  } else {\r\n    ElMessage.error('图片上传失败')\r\n  }\r\n}\r\n\r\n// 处理图片移除\r\nfunction handleRemove(file, fileList) {\r\n  itemForm.imageUrl = ''\r\n}\r\n\r\n// 处理上传错误\r\nfunction handleUploadError(err, file, fileList) {\r\n  ElMessage.error('图片上传失败，请重试')\r\n  console.error('Upload error:', err)\r\n}\r\n\r\n// 关闭对话框前的处理\r\nfunction handleClose(done) {\r\n  // 检查表单是否有修改\r\n  // 如果有修改，可以提示用户确认是否放弃\r\n  done()\r\n}\r\n\r\n// 保存商品信息\r\nfunction saveItem() {\r\n  // 这里添加表单验证逻辑\r\n  console.log('保存商品信息:', itemForm)\r\n  \r\n  // 模拟API调用\r\n  setTimeout(() => {\r\n    dialogItemVisible.value = false\r\n    ElMessage.success('商品信息保存成功')\r\n  }, 500)\r\n}\r\n\r\n// 加载供应商列表\r\nfunction loadSupplyList() {\r\n  axios.get(\"http://localhost:8081/supplyList\")\r\n    .then((response) => {\r\n      supplyList.value = response.data || []\r\n    })\r\n    .catch((error) => {\r\n      console.log(error)\r\n      ElMessage.error('加载供应商列表失败')\r\n    })\r\n}\r\n\r\n// 加载产地列表\r\nfunction loadPlaceList() {\r\n  axios.get(\"http://localhost:8081/placeList\")\r\n    .then((response) => {\r\n      placeList.value = response.data || []\r\n    })\r\n    .catch((error) => {\r\n      console.log(error)\r\n      ElMessage.error('加载产地列表失败')\r\n    })\r\n}\r\n\r\n// 加载仓库列表\r\nfunction loadStoreList() {\r\n  axios.get(\"http://localhost:8081/storeList\")\r\n    .then((response) => {\r\n      storeList.value = response.data || []\r\n    })\r\n    .catch((error) => {\r\n      console.log(error)\r\n      ElMessage.error('加载仓库列表失败')\r\n    })\r\n}\r\n\r\n// 加载品牌列表\r\nfunction loadBrandList() {\r\n  axios.get(\"http://localhost:8081/brandList\")\r\n    .then((response) => {\r\n      brandList.value = response.data || []\r\n    })\r\n    .catch((error) => {\r\n      console.log(error)\r\n      ElMessage.error('加载品牌列表失败')\r\n    })\r\n}\r\n\r\n// 加载