ect=function(index){\n    console.log(index);\n    //动态该currentComponent赋值,从组件集合views中获取的组件名字\n    currentComponent.value=views[index];\n  }\n\n  onMounted(function () {\n    axios.get(\"http://localhost:8081/listMenus\")\n    .then((response)=>{\n      console.log(response);\n      menus.value=response.data;\n    })\n    .catch((error)=>{\n      console.log(error);\n    })\n  })\n\n</script>\n\n<style scoped>\n  .top{\n    background-color: azure;\n  }\n  .left{\n    background-color: blanchedalmond;\n    height: 600px;\n  }\n  .right{\n    background-color: cornsilk;\n  }\n\n</style>"],"mappings":"AAkEE,OAAOA,WAAW,MAAM,qBAAqB;AAC7C,OAAOC,YAAY,MAAM,sBAAsB;AAC/C,SAAQC,SAAS,EAAEC,GAAG,QAAO,KAAK;AAClC,OAAOC,KAAK,MAAM,OAAO;;AAEzB;;;;;;;;IACA,MAAMC,KAAK,GAAC,CAACL,WAAW,EAACC,YAAY;IACrC;IACA,MAAMK,gBAAgB,GAACH,GAAG,CAACE,KAAK,CAAC,CAAC,CAAC,CAAC;IACpC,MAAOE,KAAK,GAACJ,GAAG,CAAC,EAAE,CAAC;IACpB;IACA,MAAOK,aAAa,GAAC,SAAAA,CAASC,KAAK,EAAC;MAClCC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MAClB;MACAH,gBAAgB,CAACM,KAAK,GAACP,KAAK,CAACI,KAAK,CAAC;IACrC;IAEAP,SAAS,CAAC,YAAY;MACpBE,KAAK,CAACS,GAAG,CAAC,iCAAiC,EAC1CC,IAAI,CAAEC,QAAQ,IAAG;QAChBL,OAAO,CAACC,GAAG,CAACI,QAAQ,CAAC;QACrBR,KAAK,CAACK,KAAK,GAACG,QAAQ,CAACC,IAAI;MAC3B,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAG;QACdR,OAAO,CAACC,GAAG,CAACO,KAAK,CAAC;MACpB,CAAC;IACH,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}