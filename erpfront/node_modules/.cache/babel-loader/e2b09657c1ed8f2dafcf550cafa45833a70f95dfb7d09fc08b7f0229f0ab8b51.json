{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { onMounted, reactive, ref } from 'vue';\nimport { Plus, Delete, ZoomIn } from '@element-plus/icons-vue';\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\nexport default {\n  __name: 'ItemManager',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const dialogItemVisible = ref(false);\n    const itemForm = reactive({\n      id: '',\n      itemNum: '',\n      itemName: '',\n      typeId: '',\n      store: 0,\n      brandId: '',\n      storeId: '',\n      supplyId: '',\n      placeId: '',\n      unitId: '',\n      price: 0.0,\n      sellPrice: 0.0,\n      vipPrice: 0.0,\n      itemDesc: '',\n      itemDate: '',\n      endDate: '',\n      imgs: '',\n      imgArr: []\n    });\n    function openItemDialog() {\n      dialogItemVisible.value = true;\n      loadSupplyList();\n      loadPlaceList();\n      loadStoreList();\n      loadBrandList();\n      loadUnitList();\n      getItemCode();\n    }\n    const dialogImageUrl = ref(null);\n    const dialogVisible = ref(false);\n    function handlePictureCardPreview(file) {\n      console.log(\"============\" + file);\n      dialogImageUrl.value = file.url;\n      dialogVisible.value = true;\n    }\n    function handlAvatarSuccess(path) {\n      console.log(path);\n      itemForm.imgArr.push(path);\n    }\n    const supplyList = ref([]);\n    const placeList = ref([]);\n    const storeList = ref([]);\n    const brandList = ref([]);\n    const unitList = ref([]);\n    function loadSupplyList() {\n      axios.get(\"http://localhost:8081/supplyList\").then(response => {\n        supplyList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function loadPlaceList() {\n      axios.get(\"http://localhost:8081/placeList\").then(response => {\n        placeList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function loadStoreList() {\n      axios.get(\"http://localhost:8081/storeList\").then(response => {\n        storeList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function loadBrandList() {\n      axios.get(\"http://localhost:8081/brandList\").then(response => {\n        brandList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function loadUnitList() {\n      axios.get(\"http://localhost:8081/unitList\").then(response => {\n        unitList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const typeDialog = ref(false);\n    function openTypeDialog() {\n      typeDialog.value = true;\n      itemCategoryTree();\n    }\n    const config = {\n      id: 'id',\n      label: 'label',\n      children: 'children'\n    };\n    const treeRef = ref(null);\n    const categoryList = ref([]);\n    function itemCategoryTree() {\n      axios.get(\"http://localhost:8081/categoryList\").then(response => {\n        categoryList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    var pnode = null;\n    function handleClickNode(node, data) {\n      pnode = node;\n    }\n    function saveType() {\n      if (!(pnode && pnode.children.length == 0)) {\n        ElMessage(\"必选选中叶子节点\");\n        return;\n      }\n      typeDialog.value = false;\n      itemForm.typeId = pnode.id;\n    }\n    function getItemCode() {\n      axios.get(\"http://localhost:8081/getCode\").then(response => {\n        itemForm.itemNum = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function saveItemForm() {\n      //处理表单中的数组,将数组转化为字符，赋值给imgs\n      itemForm.imgs = itemForm.imgArr.toString();\n      console.log(itemForm);\n      axios.post(\"http://localhost:8081/addItem\", itemForm).then(response => {\n        if (response.data.code == 200) {\n          dialogItemVisible.value = false;\n        }\n        ElMessage(response.data.msg);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    const parentBorder = ref(true);\n    const childBorder = ref(false);\n    const itemList = ref([]);\n    const total = ref(0);\n    function queryItemList(pageNum) {\n      axios.get(\"http://localhost:8081/itemList?pageNum=\" + pageNum).then(response => {\n        itemList.value = response.data.items;\n        total.value = response.data.total;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function doImgs(path) {\n      return (path + \"\").split(\",\")[0];\n    }\n    function toStatue(statue) {\n      switch (statue) {\n        case 0:\n          return \"上架\";\n        case 1:\n          return \"下架\";\n        default:\n          return \"删除\";\n      }\n    }\n    function handlerPageChange(pageNum) {\n      queryItemList(pageNum);\n    }\n    onMounted(function () {\n      queryItemList(1);\n    });\n    const __returned__ = {\n      dialogItemVisible,\n      itemForm,\n      openItemDialog,\n      dialogImageUrl,\n      dialogVisible,\n      handlePictureCardPreview,\n      handlAvatarSuccess,\n      supplyList,\n      placeList,\n      storeList,\n      brandList,\n      unitList,\n      loadSupplyList,\n      loadPlaceList,\n      loadStoreList,\n      loadBrandList,\n      loadUnitList,\n      typeDialog,\n      openTypeDialog,\n      config,\n      treeRef,\n      categoryList,\n      itemCategoryTree,\n      get pnode() {\n        return pnode;\n      },\n      set pnode(v) {\n        pnode = v;\n      },\n      handleClickNode,\n      saveType,\n      getItemCode,\n      saveItemForm,\n      parentBorder,\n      childBorder,\n      itemList,\n      total,\n      queryItemList,\n      doImgs,\n      toStatue,\n      handlerPageChange,\n      onMounted,\n      reactive,\n      ref,\n      get Plus() {\n        return Plus;\n      },\n      get Delete() {\n        return Delete;\n      },\n      get ZoomIn() {\n        return ZoomIn;\n      },\n      get axios() {\n        return axios;\n      },\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","reactive","ref","Plus","Delete","ZoomIn","axios","ElMessage","dialogItemVisible","itemForm","id","itemNum","itemName","typeId","store","brandId","storeId","supplyId","placeId","unitId","price","sellPrice","vipPrice","itemDesc","itemDate","endDate","imgs","imgArr","openItemDialog","value","loadSupplyList","loadPlaceList","loadStoreList","loadBrandList","loadUnitList","getItemCode","dialogImageUrl","dialogVisible","handlePictureCardPreview","file","console","log","url","handlAvatarSuccess","path","push","supplyList","placeList","storeList","brandList","unitList","get","then","response","data","catch","error","typeDialog","openTypeDialog","itemCategoryTree","config","label","children","treeRef","categoryList","pnode","handleClickNode","node","saveType","length","saveItemForm","toString","post","code","msg","parentBorder","childBorder","itemList","total","queryItemList","pageNum","items","doImgs","split","toStatue","statue","handlerPageChange"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/ItemManager.vue"],"sourcesContent":["<template>\r\n  <h2>商品信息</h2>\r\n\r\n  <div style=\"text-align: left\">\r\n    <el-button type=\"primary\"  @click=\"openItemDialog\">添加商品</el-button>\r\n  </div>\r\n\r\n  <el-dialog v-model=\"dialogItemVisible\" width=\"80%\" class=\"custom-dialog\">\r\n    \r\n    <div style=\"text-align: left\">\r\n      商品图片:<!--  上传商品图片控件  -->\r\n      <el-upload action=\"http://localhost:8081/uploadImg\" list-type=\"picture-card\" :auto-upload=\"true\" method=\"post\" :on-success=\"handlAvatarSuccess\">\r\n        <el-icon>\r\n          <Plus/>\r\n        </el-icon>\r\n\r\n        <template #file=\"{ file }\">\r\n          <div>\r\n            <img class=\"el-upload-list__item-thumbnail\" :src=\"file.url\" alt=\"\"/>\r\n            <span class=\"el-upload-list__item-actions\">\r\n          <span\r\n              class=\"el-upload-list__item-preview\"\r\n              @click=\"handlePictureCardPreview(file)\"\r\n          >\r\n            <el-icon><zoom-in/></el-icon>\r\n          </span>\r\n        </span>\r\n          </div>\r\n        </template>\r\n      </el-upload>\r\n    </div>\r\n\r\n  <el-form :model=\"itemForm\" label-width=\"100px\" style=\"max-width: 800px; margin-top: 20px;\">\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"名称\" required>\r\n          <el-input v-model=\"itemForm.itemName\" placeholder=\"请输入商品名称\" />\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"编号\" required>\r\n          <el-input v-model=\"itemForm.itemNum\" placeholder=\"请输入商品编号\" />\r\n        </el-form-item>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"种类\" required>\r\n          <el-input v-model=\"itemForm.typeId\" placeholder=\"选择类型\" @click=\"openTypeDialog\"/>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"库存\">\r\n          <el-input-number v-model=\"itemForm.store\" :min=\"0\" />\r\n        </el-form-item>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"品牌\" required>\r\n          <el-select v-model=\"itemForm.brandId\" placeholder=\"请选择品牌\">\r\n            <el-option\r\n            v-for=\"item in brandList\"\r\n            :key=\"item.brandId\"\r\n            :label=\"item.brandName\"\r\n            :value=\"item.brandId\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"仓库\" required>\r\n          <el-select v-model=\"itemForm.storeId\" placeholder=\"请选择仓库\">\r\n            <el-option\r\n            v-for=\"item in storeList\"\r\n            :key=\"item.storeId\"\r\n            :label=\"item.storeName\"\r\n            :value=\"item.storeId\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"供应商\" required>\r\n          <el-select v-model=\"itemForm.supplyId\" placeholder=\"请选择供应商\">\r\n            <el-option\r\n            v-for=\"item in supplyList\"\r\n            :key=\"item.supplyId\"\r\n            :label=\"item.supplyName\"\r\n            :value=\"item.supplyId\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"产地\" required>\r\n          <el-select v-model=\"itemForm.placeId\" placeholder=\"请选择产地\">\r\n            <el-option\r\n            v-for=\"item in placeList\"\r\n            :key=\"item.placeId\"\r\n            :label=\"item.placeName\"\r\n            :value=\"item.placeId\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-form-item label=\"单位\" required>\r\n      <el-select v-model=\"itemForm.unitId\" placeholder=\"请选择单位\">\r\n        <el-option\r\n            v-for=\"item in unitList\"\r\n            :key=\"item.unitId\"\r\n            :label=\"item.unitName\"\r\n            :value=\"item.unitId\"\r\n            />\r\n      </el-select>\r\n    </el-form-item>\r\n\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"进价\">\r\n          <el-input v-model=\"itemForm.price\" placeholder=\"0\">\r\n            <template #append>元</template>\r\n          </el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"售价\">\r\n          <el-input v-model=\"itemForm.sellPrice\" placeholder=\"0\">\r\n            <template #append>元</template>\r\n          </el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-form-item label=\"会员价\">\r\n      <el-input v-model=\"itemForm.vipPrice\" placeholder=\"0\">\r\n        <template #append>元</template>\r\n      </el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"详细介绍\">\r\n      <el-input v-model=\"itemForm.itemDesc\" type=\"textarea\" :rows=\"3\" />\r\n    </el-form-item>\r\n\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"生产日期\">\r\n          <el-date-picker v-model=\"itemForm.itemDate\" type=\"date\" placeholder=\"年/月/日\" />\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item label=\"保质期\">\r\n          <el-date-picker v-model=\"itemForm.endDate\" type=\"date\" placeholder=\"年/月/日\" />\r\n        </el-form-item>\r\n      </el-col>\r\n    </el-row>\r\n    <el-form-item>\r\n      <el-button type=\"primary\" @click=\"saveItemForm\">保存</el-button>\r\n      <el-button type=\"primary\" @click=\"cancelItemForm\">清空</el-button>\r\n    </el-form-item>\r\n  </el-form>\r\n  \r\n  </el-dialog>\r\n  <el-dialog v-model=\"dialogVisible\" class=\"preview-dialog\">\r\n    <img w-full :src=\"dialogImageUrl\" alt=\"Preview Image\" style=\"width: 100%\" />\r\n  </el-dialog>\r\n  <el-dialog\r\n      v-model=\"typeDialog\"\r\n      width=\"40%\">\r\n    <h2>选择商品类型</h2>\r\n    <el-tree\r\n        :props=\"config\"\r\n        :data=\"categoryList\"\r\n        default-expand-all\r\n        node-key=\"id\"\r\n        ref=\"treeRef\"\r\n        highlight-current=\"true\"\r\n        :expand-on-click-node=\"false\"\r\n        @node-click=\"handleClickNode\">\r\n    </el-tree>\r\n    <el-button type=\"primary\" @click=\"saveType\">确认</el-button>\r\n  </el-dialog>\r\n\r\n  <br/>\r\n\r\n  <el-table :data=\"itemList\" :border=\"parentBorder\" style=\"width: 100%\">\r\n    <el-table-column type=\"expand\">\r\n      <template #default=\"props\">\r\n        <div m=\"4\">\r\n            <el-image :src=\"doImgs(props.row.imgs)\" style=\"width: 200px;height: 100px\"></el-image>\r\n\r\n          <el-table :data=\"[props.row]\" :border=\"childBorder\">\r\n            <el-table-column label=\"进价\" prop=\"price\" />\r\n            <el-table-column label=\"销售价\" prop=\"sellPrice\" />\r\n            <el-table-column label=\"会员价\" prop=\"vipPrice\" />\r\n            <el-table-column label=\"生成日期\" prop=\"itemDate\" />\r\n            <el-table-column label=\"过期日期\" prop=\"endDate\" />\r\n            <el-table-column label=\"库存\" prop=\"store\" />\r\n          </el-table>\r\n        </div>\r\n      </template>\r\n    </el-table-column>\r\n    <el-table-column label=\"编号\" prop=\"itemNum\" />\r\n    <el-table-column label=\"名称\" prop=\"itemName\" />\r\n    <el-table-column label=\"品牌\" prop=\"brandName\" />\r\n    <el-table-column label=\"产地\" prop=\"placeName\" />\r\n    <el-table-column label=\"状态\">\r\n      <template #default=\"scope\">\r\n        {{toStatue(scope.row.statue)}}\r\n      </template>\r\n    </el-table-column>\r\n    <el-table-column label=\"单位\" prop=\"unitName\" />\r\n\r\n  </el-table>\r\n<!-- 分页组件 -->\r\n  <el-pagination\r\n      small\r\n      background\r\n      :page-size=\"6\"\r\n      :pager-count=\"10\"\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\"\r\n      class=\"mt-4\" @current-change=\"handlerPageChange\"/>\r\n</template>\r\n\r\n<script setup>\r\n\r\nimport { onMounted, reactive, ref } from 'vue'\r\nimport { Plus, Delete, ZoomIn } from '@element-plus/icons-vue'\r\nimport axios from 'axios'\r\nimport { ElMessage } from 'element-plus'\r\n\r\nconst dialogItemVisible = ref(false)\r\nconst itemForm = reactive({\r\n  id: '',\r\n  itemNum:'',\r\n  itemName:'',\r\n  