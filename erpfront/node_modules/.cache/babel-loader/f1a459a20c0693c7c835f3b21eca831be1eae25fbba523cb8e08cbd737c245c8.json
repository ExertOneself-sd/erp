{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { onMounted, reactive, ref } from 'vue';\nexport default {\n  __name: 'AddMenus',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const props = {\n      id: 'id',\n      label: 'label',\n      children: 'subMenus'\n    };\n    const treeNodeList = ref([]);\n    const menuForm = reactive({\n      label: ''\n    });\n    function loadMenuTree() {\n      axios.get(\"http://localhost:8081/listMenus\").then(response => {\n        treeNodeList.value = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    onMounted(function () {\n      loadMenuTree();\n    });\n    var id = 0;\n    var url = \"\";\n    function hanldNodeClick(node) {\n      console.log(node);\n      url = \"http://localhost:8081/saveMenus\";\n      id = node.id;\n    }\n    function subMenuForm() {\n      if (id == 0) {\n        ElMessage(\"请选择当前新增菜单节点的父节点\");\n        return;\n      }\n      if (!menuForm.label || menuForm.label.trim() === '') {\n        ElMessage.error(\"菜单名称不能为空\");\n        return;\n      }\n      menuForm.pid = id;\n      axios.post(url, menuForm).then(response => {\n        if (response.data.code == 200) {\n          loadMenuTree();\n          id = 0;\n        }\n        ElMessage(response.data.msg);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n    function delMenus(node, data) {\n      console.log(data.subMenus);\n      if (data.subMenus.length == 0) {\n        let mid = data.id;\n        axios.get(\"http://localhost:8081/deleteMenus/\" + mid).then(response => {\n          if (response.data.code == 200) {\n            loadMenuTree();\n          }\n          ElMessage(response.data.msg);\n        });\n      } else {\n        ElMessage(data.label + \",节点存在子节点不能删除!\");\n      }\n    }\n    function showMenus(node, data) {\n      console.log();\n      menuForm.label = data.label;\n      menuForm.id = data.id;\n      id = node.parent.data.id;\n      url = \"http://localhost:8081/updateMenus\";\n    }\n    const __returned__ = {\n      props,\n      treeNodeList,\n      menuForm,\n      loadMenuTree,\n      get id() {\n        return id;\n      },\n      set id(v) {\n        id = v;\n      },\n      get url() {\n        return url;\n      },\n      set url(v) {\n        url = v;\n      },\n      hanldNodeClick,\n      subMenuForm,\n      delMenus,\n      showMenus,\n      get axios() {\n        return axios;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      onMounted,\n      reactive,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","ElMessage","onMounted","reactive","ref","props","id","label","children","treeNodeList","menuForm","loadMenuTree","get","then","response","value","data","catch","error","console","log","url","hanldNodeClick","node","subMenuForm","trim","pid","post","code","msg","delMenus","subMenus","length","mid","showMenus","parent"],"sources":["D:/Program/Java QP/Erp/erpfront/src/views/AddMenus.vue"],"sourcesContent":["<template>\r\n  <h2>添加菜单</h2>\r\n  <div>\r\n    选择新增节点的父节点：\r\n      <el-tree\r\n      :props=\"props\"\r\n      :data=\"treeNodeList\"\r\n      node-key=\"id\"\r\n      ref=\"treeRef\"\r\n      @node-click=\"hanldNodeClick\"\r\n  >\r\n          <template #default=\"{ node, data }\">\r\n        <div class=\"custom-tree-node\">\r\n          <span>{{ node.label }}</span>\r\n          <div>\r\n            <el-button type=\"primary\" link @click.stop=\"showMenus(data)\">\r\n              修改\r\n            </el-button>\r\n            <el-button\r\n              style=\"margin-left: 4px\"\r\n              type=\"danger\"\r\n              link\r\n              @click.stop=\"delMenus(node, data)\"\r\n            >\r\n              删除\r\n            </el-button>\r\n          </div>\r\n        </div>\r\n      </template>\r\n\r\n    </el-tree>\r\n  </div>\r\n    <hr/>\r\n\r\n  <el-form :model=\"menuForm\" label-width=\"auto\" style=\"max-width: 600px\">\r\n  <el-form-item label=\"菜单名称\" style=\"margin-bottom: 24px\">\r\n    <el-input \r\n      v-model=\"menuForm.label\" />\r\n  </el-form-item>\r\n  <el-form-item style=\"text-align: right; margin-top: 24px\">\r\n    <el-button type=\"primary\" @click=\"subMenuForm\">确定</el-button>\r\n    <el-button >取消</el-button>\r\n  </el-form-item>\r\n</el-form>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\nimport { onMounted, reactive, ref } from 'vue';\r\n\r\n\r\nconst props = {\r\n  id: 'id',\r\n  label: 'label',\r\n  children: 'subMenus'\r\n}\r\n\r\nconst treeNodeList = ref([])\r\nconst menuForm = reactive({\r\n    label:''\r\n})\r\n\r\nfunction loadMenuTree() {\r\n  axios.get(\"http://localhost:8081/listMenus\")\r\n  .then((response)=>{\r\n    treeNodeList.value = response.data\r\n  })\r\n  .catch((error)=>{\r\n    console.log(error)\r\n  })\r\n}\r\n\r\nonMounted(function(){\r\n  loadMenuTree()\r\n})\r\n\r\nvar id=0\r\nvar url=\"\"\r\n\r\n\r\nfunction hanldNodeClick(node){\r\n    console.log(node)\r\n    url=\"http://localhost:8081/saveMenus\"\r\n    id=node.id\r\n}\r\n\r\nfunction subMenuForm(){\r\n    if(id==0){\r\n        ElMessage(\"请选择当前新增菜单节点的父节点\")\r\n        return\r\n    }\r\n\r\n    if(!menuForm.label || menuForm.label.trim() === ''){\r\n        ElMessage.error(\"菜单名称不能为空\")\r\n        return\r\n    }\r\n    menuForm.pid=id\r\n    axios.post(url,menuForm)\r\n    .then((response)=>{\r\n     if(response.data.code==200){\r\n        loadMenuTree()\r\n        id=0\r\n\r\n     }\r\n     ElMessage(response.data.msg)\r\n    })\r\n    .catch((error)=>{\r\n        console.log(error)\r\n    })\r\n}\r\n\r\n\r\nfunction delMenus(node,data){\r\n    console.log(data.subMenus)\r\n    if(data.subMenus.length==0){\r\n        let mid=data.id\r\n        axios.get(\"http://localhost:8081/deleteMenus/\"+mid)\r\n        .then((response)=>{\r\n            if(response.data.code==200){\r\n                loadMenuTree()\r\n            }\r\n            ElMessage(response.data.msg)\r\n        })\r\n    }else{\r\n        ElMessage(data.label+\",节点存在子节点不能删除!\")\r\n    }\r\n}\r\n\r\nfunction showMenus(node,data){\r\n    console.log()\r\n    menuForm.label=data.label\r\n    menuForm.id=data.id\r\n    id=node.parent.data.id\r\n    url=\"http://localhost:8081/updateMenus\"\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.custom-tree-node {\r\n  flex: 1;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  font-size: 14px;\r\n  padding-right: 8px;\r\n}\r\n</style>"],"mappings":"AA+CA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,cAAc;AACxC,SAASC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAQ,KAAK;;;;;;;IAG9C,MAAMC,KAAK,GAAG;MACZC,EAAE,EAAE,IAAI;MACRC,KAAK,EAAE,OAAO;MACdC,QAAQ,EAAE;IACZ,CAAC;IAED,MAAMC,YAAY,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMM,QAAQ,GAAGP,QAAQ,CAAC;MACtBI,KAAK,EAAC;IACV,CAAC,CAAC;IAEF,SAASI,YAAYA,CAAA,EAAG;MACtBX,KAAK,CAACY,GAAG,CAAC,iCAAiC,CAAC,CAC3CC,IAAI,CAAEC,QAAQ,IAAG;QAChBL,YAAY,CAACM,KAAK,GAAGD,QAAQ,CAACE,IAAI;MACpC,CAAC,CAAC,CACDC,KAAK,CAAEC,KAAK,IAAG;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;IAEAhB,SAAS,CAAC,YAAU;MAClBS,YAAY,CAAC,CAAC;IAChB,CAAC,CAAC;IAEF,IAAIL,EAAE,GAAC,CAAC;IACR,IAAIe,GAAG,GAAC,EAAE;IAGV,SAASC,cAAcA,CAACC,IAAI,EAAC;MACzBJ,OAAO,CAACC,GAAG,CAACG,IAAI,CAAC;MACjBF,GAAG,GAAC,iCAAiC;MACrCf,EAAE,GAACiB,IAAI,CAACjB,EAAE;IACd;IAEA,SAASkB,WAAWA,CAAA,EAAE;MAClB,IAAGlB,EAAE,IAAE,CAAC,EAAC;QACLL,SAAS,CAAC,iBAAiB,CAAC;QAC5B;MACJ;MAEA,IAAG,CAACS,QAAQ,CAACH,KAAK,IAAIG,QAAQ,CAACH,KAAK,CAACkB,IAAI,CAAC,CAAC,KAAK,EAAE,EAAC;QAC/CxB,SAAS,CAACiB,KAAK,CAAC,UAAU,CAAC;QAC3B;MACJ;MACAR,QAAQ,CAACgB,GAAG,GAACpB,EAAE;MACfN,KAAK,CAAC2B,IAAI,CAACN,GAAG,EAACX,QAAQ,CAAC,CACvBG,IAAI,CAAEC,QAAQ,IAAG;QACjB,IAAGA,QAAQ,CAACE,IAAI,CAACY,IAAI,IAAE,GAAG,EAAC;UACxBjB